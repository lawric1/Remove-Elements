{
    "manifest_version": 2,

    "name": "Element Remover",
    "description": "This extension will remove clicked element from the page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}